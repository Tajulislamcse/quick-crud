{
  "name": "tajulislamcse/quick-crud",
  "description": "A Laravel CRUD generator package",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Tajul Islam",
      "email": "tajulislam8028@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
     "illuminate/support": "^9.0 ||^10.0 || ^11.0"
  },
  "autoload": {
    "psr-4": {
      "QuickCrud\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "QuickCrud\\QuickCrudServiceProvider"
      ]
    }
  },
  "repositories": [
  {
    "type": "vcs",
    "url": "https://github.com/Tajulislamcse/quick-crud"
  }
],
  "minimum-stability": "dev",
  "prefer-stable": true
}
